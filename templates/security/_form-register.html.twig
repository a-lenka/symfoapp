{% form_theme form 'themes/materialize-fields.html.twig' %}

<h4>{{ 'Registration'|trans }}</h4>

{{ form_start(form, {'action': path('register')}) }}
    {{ form_row(form.email) }}

    {{ form_row(form.password) }}

    <div class="modal-footer row right-align">
        {{ form_row(form.termsAccepted) }}

        {{ form_row(form.submit) }}
    </div>
{{ form_end(form) }}
